{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "es2020"],
    "outDir": "dist",
    "noImplicitAny": true,
    "allowJs": true,
    "strictNullChecks": true,
    "declaration": true,
    "skipLibCheck": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
